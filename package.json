{
  "name": "mongo-repo",
  "version": "1.0.0",
  "description": "This module is a thin wrapper around mongodb native driver that allows you to easily derive basic collection operations and share a pooled connection in a Node application.",
  "main": "index.js",
  "scripts": {
    "test": "mocha $@"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:IWSLLC/mongo-repo.git"
  },
  "keywords": [
    "mongodb",
    "helper",
    "collection",
    "shared",
    "pool"
  ],
  "author": "Nathan Bridgewater",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/IWSLLC/mongo-repo/issues"
  },
  "homepage": "https://github.com/IWSLLC/mongo-repo",
  "mocha": {
    "reporter": "spec",
    "ui": "bdd",
    "recursive": true,
    "colors": true,
    "exit": true,
    "delay": true,
    "parallel": false,
    "extension": [
      "js"
    ]
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "^2.23.4",
    "mocha": "^9.0.0",
    "should": "^13.2.3",
    "sinon": "^11.1.1"
  },
  "dependencies": {
    "async": "^3.2.0",
    "coffeescript": "^2.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^3.6.9"
  }
}
